<!DOCTYPE html>
#// 1
<html lang="en">
	<head>
		<meta charset="utf-8" />
		#// 2
		<title>#(title) | Todo List</title>
	</head>
	<body>
		#// 3
		<h1>#(title) | Todo List</h1>
		<table>
			#if(todoList) {
			#// 3
			<table>
				<thead>
					<tr>
						<th>task</th>
						<th>duedate</th>
					</tr>
				</thead>
				<tbody>
					#// 4
					#for(todo in todoList) {
					<tr>
						#// 5
						<td>#(todo.task)</td>
						<td>#(todo.duedate)</td>
					</tr>
					}
				</tbody>
			</table>
			#// 6
			} else {
			<h2>There arenâ€™t any todo yet!</h2>
			}
	</body>
</html>
